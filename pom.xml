<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.github</groupId>
	<artifactId>simpledatax-service</artifactId>
	<version>${project-version}</version>
	<packaging>jar</packaging>
	<name>simpledatax-service</name>
	<url>http://maven.apache.org</url>

	<build>
		<finalName>simpledatax-service</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>


	<properties>
		<project-version>1.0.0.0-SNAPSHOT</project-version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<slf4j.version>1.7.25</slf4j.version>
		<spring.version>4.3.14.RELEASE</spring.version>
		<fastjson.version>1.2.83</fastjson.version>
		<druid.version>1.0.29</druid.version>
		<oracle10.version>10.2.0.1.0</oracle10.version>
		<orai18n.version>10.2.0.1</orai18n.version>
		<sqljdbc4.version>4.0</sqljdbc4.version>
		<guava.version>16.0.1</guava.version>
		<sqlite-jdbc.version>3.7.2</sqlite-jdbc.version>
		<javacsv.version>2.0</javacsv.version>
		<groovy-all.version>2.1.9</groovy-all.version>

		<!-- commons tools settings -->
		<poi.version>3.9</poi.version>
		<xml-apis.version>1.4.01</xml-apis.version>
		<commons-lang3.version>3.3.2</commons-lang3.version>
		<commons-io.version>2.4</commons-io.version>
		<commons-codec.version>1.9</commons-codec.version>
		<commons-fileupload.version>1.3.1</commons-fileupload.version>
		<commons-beanutils.version>1.9.1</commons-beanutils.version>
		<commons-math3.version>3.1.1</commons-math3.version>
		<commons-configuration.version>1.10</commons-configuration.version>
		<commons-cli.version>1.2</commons-cli.version>
		<httpclient.version>4.5.6</httpclient.version>
		<fluent-hc.version>4.5.6</fluent-hc.version>
		<commons-collections.version>3.2.1</commons-collections.version>
		<commons-compress.version>1.9</commons-compress.version>
		<commons-net.version>3.3</commons-net.version>
		<commons-dbcp.version>1.2.2</commons-dbcp.version>

		<junit.version>4.12</junit.version>

		<maven.compiler.source>1.6</maven.compiler.source>
		<maven.compiler.target>1.6</maven.compiler.target>
	</properties>

